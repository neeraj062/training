
public class CalcMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=10,b=30;
		Calculator cal = new Calculator();
		System.out.println("Added Integer Value = "+cal.add(a, b));
		
		System.out.println("Added Float Value = "+cal.add(10.8f, 11.4f));
		
		System.out.println("Added Float Value = "+cal.add("FirstName", "LastName"));
		
		
	}

}
