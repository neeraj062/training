import java.util.*;
class Calculator
{
	/*----------Method for Addition of two numbers-------------------*/
	
	static int add(int a,int b){
		return a+b;
	}


	/*-------------------Driver Function-----------------------------*/
	
	public static void main(String[] args){
	Scanner sc = new Scanner(System.in);
	int a=0,b=0;
	if(sc.hasNextInt())
		a= sc.nextInt();
	if(sc.hasNextInt())
		b= sc.nextInt();
	
	System.out.println("Added Value = " +Test.add(a,b));
	sc.close();
	}
}